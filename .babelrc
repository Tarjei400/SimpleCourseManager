{
  "presets": [ "stage-2", "stage-3", "es2015", "es2016", "es2017", "react"],
  "plugins": ["transform-runtime", "transform-decorators-legacy",
    [ "module-resolver",
      {
        "root": [
          "./node_modules",
          "./client",
          "./server",
          "./env"
        ],
        "alias": {
          "config": "./env/config"
        }
      }]

    ]
}
